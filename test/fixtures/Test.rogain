<div>
  <{tag} no-children />
  <TestBlock tagName={tag} {data}={colors|first} />
  <TestComponent id="colors">
    <TestBlock data={colors|last} />
    <a href="/colors">Read More</a>
  </TestComponent>
  <{component} data={colors} as="color">
    <TestBlock data={color} />
    <Defined data={color.slug}>
      <a href="/colors/{color.slug}">Read More</a>
    </Defined>
  </{component}>
  <script src={scripts.main}>
    function hello(str) { }
  </script>
</div>